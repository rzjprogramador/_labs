{
    "env": {
        "es2021": true,
        "node": true,
        "vitest": true,
        "jest": true
    },
    "extends": [
        "standard",
        "prettier",
        "plugin:prettier/recommended",
        "some-other-config-you-use",
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint",
        "prettier"
    ],
    "rules": {
        "prettier/prettier": "error"
    }
}
