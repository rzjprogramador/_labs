{
    "name": "Api",
    "version": "1.0.0",
    "main": "index.js",
    "repository": "",
    "author": "Reinaldo <60945992+rzjprogramador@users.noreply.github.com>",
    "license": "MIT",
    "scripts": {
        "dev": "ts-node-dev --transpile-only --respawn --ignore-watch node_modules -r tsconfig-paths/register src/_server/server.ts",
        "nao:reset:prod": "npx dotenv-cli -e .env -- npx prisma migrate reset --force --skip-generate --preview-feature",
        "reset:dev": "npx dotenv-cli -e .env.dev -- npx prisma migrate reset --force --skip-generate --preview-feature",
        "reset:test": "npx dotenv-cli -e .env.test -- npx prisma migrate reset --force --skip-generate --preview-feature",
        "lint": "eslint src/** --fix .",
        "test": "jest --passWithNoTests --silent --noStackTrace --runInBand",
        "test:verbose": "jest --passWithNoTests --runInBand",
        "test:unit": "yarn test --watch -c jest-unit-config.js",
        "test:integration": "yarn test -- --watch -c jest-integration-config.js",
        "test:clear": "jest --clearCache",
        "test:staged": "yarn test --findRelatedTests",
        "test:ci": "yarn test --coverage",
        "test:w": "yarn test --watch",
        "build": "tsc -p tsconfig-build.json"
    },
    "devDependencies": {
        "@prisma/client": "^3.14.0",
        "@types/express": "^4.17.13",
        "@types/jest": "^27.4.0",
        "@types/node": "^17.0.9",
        "@types/validator": "^13.7.1",
        "@typescript-eslint/eslint-plugin": "^5.10.0",
        "@typescript-eslint/parser": "^5.10.0",
        "eslint": "^7.12.1",
        "eslint-config-standard": "^16.0.3",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^4.2.1",
        "git-commit-msg-linter": "^4.0.7",
        "husky": "^7.0.4",
        "jest": "^27.4.7",
        "lint-staged": "^12.3.3",
        "mockdate": "^3.0.5",
        "ts-jest": "^27.1.3",
        "ts-node-dev": "^1.1.8",
        "tsconfig-paths": "^3.12.0",
        "typescript": "^4.5.4"
    },
    "dependencies": {
        "dotenv": "^16.0.1",
        "express": "^4.18.1",
        "module-alias": "^2.2.2",
        "prisma": "^3.14.0",
        "validator": "^13.7.0"
    },
    "_moduleAliases": {
        "@": "dist"
    },
    "prisma": {
        "seed": "ts-node prisma/seed.ts"
    }
}
