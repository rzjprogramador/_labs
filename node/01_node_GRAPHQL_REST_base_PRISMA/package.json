{
  "name": "node_base_01",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev --respawn --watch 'src/**' --transpile-only --ignore-watch node_modules -r tsconfig-paths/register  src/_server/server.ts ",
    "build": "tsc -p tsconfig-build.json",
    "nao:reset:prod": "npx dotenv-cli -e .env -- npx prisma migrate reset --force --skip-generate --preview-feature",
    "reset:dev": "npx dotenv-cli -e .env.dev -- npx prisma migrate reset --force --skip-generate --preview-feature",
    "reset:test": "npx dotenv-cli -e .env.test -- npx prisma migrate reset --force --skip-generate --preview-feature",
    "test": "jest --passWithNoTests --runInBand --no-cache"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/mongoose": "^5.11.97",
    "prisma": "^3.14.0",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@prisma/client": "^3.14.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-graphql": "^0.12.0",
    "graphql": "^15.5.1",
    "graphql-tools": "^7.0.5",
    "mongoose": "^5.13.2",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.6.0",
    "uuid": "^8.3.2"
  },
  "_moduleAliases": {
    "@src": "dist"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
