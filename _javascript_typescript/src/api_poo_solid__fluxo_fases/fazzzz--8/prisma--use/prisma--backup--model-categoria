// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

// MODELOS DAS TABELAS

model Categoria {
    id         String   @id @default(uuid())
    nome       String
    descricao  String
    created_at DateTime @default(now())

}
